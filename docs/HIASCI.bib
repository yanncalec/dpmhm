	file = {ScienceDirect Full Text PDF:/home/han/Zotero/storage/JQC9ZC8R/Lei et al. - 2020 - Applications of machine learning to machine fault .pdf:application/pdf;ScienceDirect Snapshot:/home/han/Zotero/storage/23RXSYWI/S0888327019308088.html:text/html},
}

@article{zhang_intelligent_2022,
	title = {Intelligent fault diagnosis of machines with small \& imbalanced data: {A} state-of-the-art review and possible extensions},
	volume = {119},
	issn = {0019-0578},
	shorttitle = {Intelligent fault diagnosis of machines with small \& imbalanced data},
