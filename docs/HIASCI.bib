	file = {Full Text PDF:/home/han/Zotero/storage/L664BMMB/Tanabe et al. - 2021 - MIMII DUE Sound Dataset for Malfunctioning Indust.pdf:application/pdf;Snapshot:/home/han/Zotero/storage/B4JZ6DYJ/2105.html:text/html},
